   "source": [
    "from scipy.stats import shapiro\n",
    "data = [75, 83, 81, 92, 68, 77, 78, 80, 85, 95, 79, 89]\n",
    "statistic, pvalue = shapiro(data)\n",
    "print(statistic, pvalue)\n",
    "\n",
    "alpha = 0.05\n",
    "if pvalue < alpha:\n",
    "    print(\"H0  귀무가설을 기각합니다, 데이터는 정규분포를 따르지 않음\")\n",
    "else:\n",
    "    print(\"H0 귀무가설을 기각할 수 없습니다. 데이터는 정규분포를 따름\")\n"
   ]
  }
